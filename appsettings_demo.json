{
  "SabatexSettings": {
    "TempFolder": "C:\\Users\\<user>\\AppData\\Local\\Temp\\Sabatex",
    "NUGET": {
      "NugetAuthTokenPath": "C:\\Secrets\\nuget.token",
      "LocalDebugStorage": "C:\\LocalNuget\\Debug"
    },
    "Linux": {
      "ServiceName": "myproject",
      "UserHomeFolder": "/home/azureuser",
      "Port": 5000,
      "FrontEnd": false,
      "PublishFolder": "/var/www/myproject",
      "BitviseTlpFile": "/home/azureuser/.bitvise/myproject.tlp",
      "NGINX": {
        "HostName": [
          "example.com",
          "www.example.com"
        ],
        "SSLPublic": "/etc/ssl/certs/example.crt",
        "SSLPrivate": "/etc/ssl/private/example.key",
        "AppPort": 5000
      }
    }
  }
}